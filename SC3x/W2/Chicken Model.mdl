{UTF-8}
Chickens= INTEG (
	Hatched-Expire,
		10)
	~	
	~		|

Chickens to Cross Road=
	Chickens
	~	
	~		|

Egg Lay Rate=
	5
	~	
	~		|

Eggs= INTEG (
	Eggs Layed-Hatched-Lost,
		0)
	~	
	~		|

Eggs Layed=
	Chickens*Egg Lay Rate
	~	
	~		|

Eggs to Hatch=
	DELAY FIXED( Eggs Layed , 3 , 0 )
	~	
	~		|

Expire=
	Chickens to Cross Road*Expire Rate
	~	
	~		|

Expire Rate=
	0.1
	~	
	~		|

Hatch Rate=
	IF THEN ELSE( Chickens>20 , 0 , 0.05 )
	~	
	~		|

Hatched=
	Eggs to Hatch*Hatch Rate
	~	
	~		|

Lost=
	Eggs to Hatch*(1-Hatch Rate)
	~	
	~		|

