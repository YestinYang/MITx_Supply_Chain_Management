{UTF-8}
Adjustment=
	Target Service Level-Service Level
	~	
	~		|

Adjustment to S= INTEG (
	Adjustment,
		0)
	~	
	~		|

Delay=
	100
	~	
	~		|

Delayed Demand Filled=
	DELAY FIXED( Demand Filled , Delay , 0 )
	~	
	~		|

Delayed Demand Lost=
	DELAY FIXED( Demand Lost , Delay , 0 )
	~	
	~		|

Demand Filled=
	MIN( Inventory , Random Demand )
	~	
	~		|

Demand Lost=
	MAX( Random Demand-Inventory , 0 )
	~	
	~		|

Inventory= INTEG (
	Orders-Sales,
		25)
	~	
	~		|

Items Sold= INTEG (
	Sales-Obsolate Sales,
		0)
	~	
	~		|

Lost Sales= INTEG (
	Sales Lost-Obsolate Lost Sales,
		0)
	~	
	~		|

Obsolate Lost Sales=
	Delayed Demand Lost
	~	
	~		|

Obsolate Sales=
	Delayed Demand Filled
	~	
	~		|

Order=
	IF THEN ELSE( Inventory<S , Q , 0 )
	~	
	~		|

Orders=
	Order
	~	
	~		|

Q=
	100
	~	
	~		|

Random Demand=
	RANDOM NORMAL( 0 , 100 , 20 , 10 , 0 )
	~	
	~		|

S=
	20+Adjustment to S
	~	
	~		|

Sales=
	Demand Filled
	~	
	~		|

Sales Lost=
	Demand Lost
	~	
	~		|

Service Level=
	IF THEN ELSE( Items Sold+Lost Sales>1 , Items Sold/(Items Sold+Lost Sales) , 1 )
	~	
	~		|

Target Service Level=
	0.95
	~	
	~		|

Total Items Sold= INTEG (
	Obsolate Sales,
		0)
	~	
	~		|

Total Lost Sales= INTEG (
	Obsolate Lost Sales,
		0)
	~	
	~		|

